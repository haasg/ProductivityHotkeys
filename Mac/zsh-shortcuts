alias home="cd ~"
alias repos="cd ~/Documents/repo"
alias alias-open="code ~/.zshrc"
alias alias-save="source ~/.zshrc"
alias hammer-open="code ~/.hammerspoon"
alias env-open="code ~/.zshenv"
alias env-save="source ~/.zshenv"
alias gnuke="git clean -df; git reset HEAD --hard"
alias greset="git reset HEAD~1"
alias gsta="git stash --include-untracked"
alias gpp="git stash pop"
alias podi="pod install"
alias podu="pod update"
